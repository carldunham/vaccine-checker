  "version": "0.2.0",
  "configurations": [
    {
      "name": "Normal Running",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["--distance", "200"]
    },
    {
      "name": "Silent Running",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["--silent", "--distance", "200"]
    },
    {
      "name": "Invalid Param",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["--foo"]
    }
  ]
}
